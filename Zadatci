SELECT SUBSTRING(Ime, 1, 1) AS "Ime", SUBSTRING (Prezime, 1, 1) AS "Prezime", datRod
FROM student

SELECT *
FROM student
ORDER BY(datRod)

SELECT TOP 1 ime, prezime
FROM student
WHERE spol='F'
ORDER BY datRod



2.

SELECT COUNT(mbr)
FROM student

SELECT COUNT(DISTINCT pbrPreb)
FROM student

